<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">

        <record id="create_monthly_sale_order_proposal" model="ir.cron">
            <field name="name">Create Monthly Sale Order Proposal</field>
            <field eval="False" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">months</field>
<!--            <field name="numbercall">-1</field>-->
            <field name="model_id" ref="model_res_partner"/>
            <field name="state">code</field>
            <field name="code">model.create_monthly_proposal(customer_ids=[])</field>
        </record>

        <record id="create_interval_monthly_sale_order_proposal" model="ir.cron">
            <field name="name">Create Interval Monthly Sale Order Proposal</field>
            <field eval="False" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">months</field>
<!--            <field name="numbercall">-1</field>-->
            <field name="model_id" ref="model_res_partner"/>
            <field name="state">code</field>
            <field name="code">model.create_interval_proposal()</field>
        </record>
    </data>
</odoo>