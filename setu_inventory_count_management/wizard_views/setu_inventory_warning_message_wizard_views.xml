<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="setu_inventory_warning_message_wizard_form_view" model="ir.ui.view">
            <field name="name">setu.inventory.warning.message.wizard.form.view</field>
            <field name="model">setu.inventory.warning.message.wizard</field>
            <field name="arch" type="xml">
                <form string="Warning!!!">
                    <sheet>
                        <field name="message" readonly="1" nolabel="1"/>
                    </sheet>
                    <footer>
                        <button name="approve" class="oe_highlight" type="object" string="Confirm"/>
                        <button special="cancel" string="Close"/>
                    </footer>
                </form>
            </field>
        </record>

