<?xml version='1.0' encoding='UTF-8' ?>
<odoo>
    <record id="setu_humand_integration_inherited_REF_hr_leave_view_form" model="ir.ui.view">
        <field name="name">setu.humand.integration.inherited.REF.hr.leave.view.form</field>
        <field name="model">hr.leave</field>
        <field name="inherit_id" ref="hr_holidays.hr_leave_view_form"/>
