<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ict_stock_picking_form" model="ir.ui.view">
            <field name="name">ict.stock.picking.form</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <field name="origin" position="after">
                    <field name="ict_internal_transfer_id" readonly="1" invisible="not ict_internal_transfer_id"/>
                    <field name="sale_intercompany_transfer_id" readonly="1"
                           invisible="not sale_intercompany_transfer_id"/>
                    <field name="purchase_intercompany_transfer_id" readonly="1"
                           invisible="not purchase_intercompany_transfer_id"/>
                </field>
            </field>
        </record>
    </data>
</odoo>