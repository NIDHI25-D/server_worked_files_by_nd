<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="competition_level_tree_view" model="ir.ui.view">
        <field name="name">competition.level.tree</field>
        <field name="model">competition.level</field>
        <field name="arch" type="xml">
            <list>
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="competition_level_form_view" model="ir.ui.view">
        <field name="name">competition.level.form.view</field>
        <field name="model">competition.level</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" required="1"/>
                        <field name="final_profit_margin"/>
                    </group>
                    <notebook>
                        <page name="Utility Profit Percent Per Product">
                            <field name="price_base_ids"/>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>


    <record id="action_competition_level" model="ir.actions.act_window">
        <field name="name">Competition Level</field>
        <field name="res_model">competition.level</field>
        <field name="view_mode">list,form</field>
    </record>

    <menuitem id="menu_competition_level" name="Competition Level"
              parent="setu_price_update.prod_price_update_menu"
              action="setu_price_update.action_competition_level"/>
</odoo>