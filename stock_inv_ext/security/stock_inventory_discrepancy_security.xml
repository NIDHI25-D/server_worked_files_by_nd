        <field name="name">Stock Move Line Deletion</field>
        <field name="category_id" ref="base.module_category_inventory_inventory"/>
    </record>
    <record id="group_stock_inventory_supervisor" model="res.groups">
        <field name="name">Inventory Supervisor</field>
        <field name="category_id" ref="base.module_category_inventory_inventory"/>
