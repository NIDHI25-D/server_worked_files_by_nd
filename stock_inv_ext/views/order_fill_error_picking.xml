<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="order_fill_error_picking_tree" model="ir.ui.view">
            <field name="name">order.fill.error.picking.tree</field>
            <field name="model">order.fill.error.picking</field>
            <field name="arch" type="xml">
                <list string="Transfer Cancellation Reason" editable="bottom">
                    <field name="name"/>
                </list>
            </field>
        </record>
    </data>


    <record id="order_fill_error_picking_action"  model="ir.actions.act_window">
            <field name="name">Transfer Cancellation Reason</field>
            <field name="res_model">order.fill.error.picking</field>
            <field name="view_mode">list</field>
        </record>

         <menuitem id="order_fill_error_picking_menu" name="Transfer Cancellation Reason"
                   parent="stock.menu_stock_config_settings"
                  action="order_fill_error_picking_action"
        />

</odoo>