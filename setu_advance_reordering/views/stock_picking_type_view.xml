<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- Inherit Form View to Modify it -->
    <record id="stock_picking_type_form_view_inherit_for_real_demand" model="ir.ui.view">
        <field name="name">stock.picking.type.form.view.inherit.for.real.demand</field>
        <field name="model">stock.picking.type</field>
        <field name="inherit_id" ref="stock.view_picking_type_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='reservation_method']" position="after">
                <field name="is_considering_in_real_demand"/>
            </xpath>
        </field>
    </record>
</odoo>