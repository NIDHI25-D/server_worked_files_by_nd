<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
     <record id="arrival_change_reason_view_tree" model="ir.ui.view">
            <field name="name">arrival.change.reason.view.tree</field>
            <field name="model">arrival.change.reason</field>
            <field name="arch" type="xml">
                <list editable="bottom">
                     <field name="name" required="1"/>
                </list>
            </field>
        </record>

        <record id="arrival_change_reason_action" model="ir.actions.act_window">
            <field name="name">Change Reason</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">arrival.change.reason</field>
            <field name="view_mode">list</field>
        </record>

        <!-- This Menu Item must have a parent and an action -->
        <menuitem id="arrival_change_reason_main_menu"
                  name="Change reason"
                  parent="purchase.menu_purchase_root"
                  sequence="99"/>

        <menuitem id="arrival_change_reason_submenu"
                  name="Change reason"
                  parent="setu_purchase_automation.arrival_change_reason_main_menu"
                  action="arrival_change_reason_action"
                  sequence="5"/>
</odoo>