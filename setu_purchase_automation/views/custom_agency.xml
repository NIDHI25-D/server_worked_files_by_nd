<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="custom_agencys_view_tree" model="ir.ui.view">
            <field name="name">custom.agencys.view.tree</field>
            <field name="model">custom.agency</field>
            <field name="arch" type="xml">
                <list editable="bottom">
                    <field name="name" required="1"/>
                    <field name="customs" required="1"/>
                    <field name="company_id"/>
                </list>
            </field>
        </record>

        <record id="custom_agency_action" model="ir.actions.act_window">
            <field name="name">Custom Agency</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">custom.agency</field>
            <field name="view_mode">list</field>
        </record>
        <menuitem id="customs_agency_menu" name="Custom Agency" parent="purchase.menu_purchase_config"
                  action="setu_purchase_automation.custom_agency_action"/>

    </data>
</odoo>