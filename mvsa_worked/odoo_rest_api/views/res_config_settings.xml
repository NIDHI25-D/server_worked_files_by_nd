<odoo>
    <data>
        <record id="res_config_settings_api_form" model="ir.ui.view">
            <field name="name">res.config.settings.api.form</field>
            <field name="model">res.config.settings</field>
            <field name="priority" eval="100"/>
            <field name="inherit_id" ref="base.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form" position="inside">
                    <app data-string="API" string="API" name="api">
                        <block title = "API">
                            <setting string="Payment Ways For REST API">
                                <field name="payment_method_ids" widget="many2many_tags" options="{'no_create': True, 'no_open': True}"/>
                            </setting>
                            <setting string="Product Category">
                                <field name="product_category_id" options="{'no_create': True, 'no_open': True}"/>
                            </setting>
                            <setting string="Get Product Prices" help="Enable this boolean api_get_product_prices">
                                <field name="api_get_product_prices"/>
                                <div class="content-group" invisible = "not api_get_product_prices">
                                    <div class="row mt16">
                                        <label for="limit_for_api_get_product_prices" string="Limit for API" class="col-lg-3"/>
                                        <field name="limit_for_api_get_product_prices"/>
                                    </div>
                                    <div class="row mt16">
                                        <label for="minutes_for_api_get_product_prices" string="Minutes for API" class="col-lg-3"/>
                                        <field name="minutes_for_api_get_product_prices"/>
                                    </div>
                                </div>
                            </setting>
                            <setting string="Get Available Stock" help="Enable this boolean get_available_stock">
                                <field name="api_get_available_stock"/>
                                <div class="content-group" invisible = "not api_get_available_stock">
                                    <div class="row mt16">
                                        <label for="limit_for_api_get_available_stock" string="Limit for API" class="col-lg-3"/>
                                        <field name="limit_for_api_get_available_stock"/>
                                    </div>
                                    <div class="row mt16">
                                        <label for="minutes_for_api_get_available_stock" string="Minutes for API" class="col-lg-3"/>
                                        <field name="minutes_for_api_get_available_stock"/>
                                    </div>
                                </div>
                            </setting>
                            <setting string="Get Category Hierarchy" help="Enable this boolean get_category_hierarchy">
                                <field name="api_get_category_hierarchy"/>
                                <div class="content-group" invisible = "not api_get_category_hierarchy">
                                    <div class="row mt16">
                                        <label for="limit_for_api_get_category_hierarchy" string="Limit for API" class="col-lg-3"/>
                                        <field name="limit_for_api_get_category_hierarchy"/>
                                    </div>
                                    <div class="row mt16">
                                        <label for="minutes_for_api_get_category_hierarchy" string="Minutes for API" class="col-lg-3"/>
                                        <field name="minutes_for_api_get_category_hierarchy"/>
                                    </div>
                                </div>
                            </setting>
                        </block>
                    </app>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
