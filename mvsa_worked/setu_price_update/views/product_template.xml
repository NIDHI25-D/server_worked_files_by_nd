<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <!-- Inherit Form View to Modify it -->
        <record id="product_template_inherit" model="ir.ui.view">
            <field name="name">product.template.inherit</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='product_tooltip']" position="after">
                    <!-- Add your fields or attributes here -->
                        <field name="import_factor_level_id" widget="selection"/>
                        <field name="competition_level_id" widget="selection"/>
                </xpath>

            </field>
        </record>
    </data>
</odoo>