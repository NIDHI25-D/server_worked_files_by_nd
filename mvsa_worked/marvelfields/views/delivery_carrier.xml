<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="website_order_mostrador_delivery_carrier_view_form" model="ir.ui.view">
        <field name="name">website.order.mostrador.delivery.carrier.view.form</field>
        <field name="model">delivery.carrier</field>
        <field name="inherit_id" ref="delivery.view_delivery_carrier_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_id']" position="before">
                <field name="is_mostrador_delivery"/>
            </xpath>
        </field>
    </record>
</odoo>