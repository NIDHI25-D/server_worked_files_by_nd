#. module: setu_send_receipt_by_email
#: model:mail.template,body_html:setu_send_receipt_by_email.vendor_payment_receipt_template
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        Dear\n"
"                        <t t-out=\"object.partner_id.name or ''\">Azure Interior</t>\n"
"                        <br>\n"
"                        <br>\n"
"                        Here is your payment receipt <span style=\"font-weight:bold;\" t-out=\"(object.name or '').replace('/','-') or ''\">\n"
"                        BNK1-2021-05-0002\n"
"                    </span> amounting\n"
"                        to <span style=\"font-weight:bold;\" t-out=\"format_amount(object.amount, object.currency_id) or ''\">$ 10.00\n"
"                    </span> from <t t-out=\"object.company_id.name or ''\">YourCompany</t>.\n"
"                        <br>\n"
"                        <br>\n"
"                        Payment Reference:\n"
"                        <t t-out=\"object.memo or ''\"></t>\n"
"                        <br>\n"
"                        Do not hesitate to contact us if you have any questions.\n"
"                        <br>\n"
"                        <br>\n"
"                        Best regards\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        Estimado\n"
"                        <t t-out=\"object.partner_id.name or ''\">Azure Interior</t>\n"
"                        <br>\n"
"                        <br>\n"
"                        Aquí está su recibo de pago <span style=\"font-weight:bold;\" t-out=\"(object.name or '').replace('/','-') or ''\">\n"
"                        BNK1-2021-05-0002\n"
"                    </span> con un importe total de\n"
"                         <span style=\"font-weight:bold;\" t-out=\"format_amount(object.amount, object.currency_id) or ''\">$ 10.00\n"
"                    </span> de <t t-out=\"object.company_id.name or ''\">YourCompany</t>.\n"
"                        <br>\n"
"                        <br>\n"
"                        Referencia de pago:\n"
"                        <t t-out=\"object.memo or ''\"></t>\n"
"                        <br>\n"
"                        No dude en contactarnos si tiene alguna duda.\n"
"                        <br>\n"
"                        <br>\n"
"                        Saludos\n"
"                    </p>\n"
"                </div>\n"
"            "

#. module: setu_send_receipt_by_email
#: model:mail.template,body_html:setu_send_receipt_by_email.mail_template_data_journal_entry
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        Dear\n"
"                        <t t-out=\"object.partner_id.name or ''\">Azure Interior</t>\n"
"                        <br>\n"
"                        <br>\n"
"                        Thank you for your payment.\n"
"                        Here is your payment receipt <span style=\"font-weight:bold;\" t-out=\"(object.name or '').replace('/','-') or ''\">\n"
"                        BNK1-2021-05-0002\n"
"                    </span> amounting\n"
"                        to <span style=\"font-weight:bold;\" t-out=\"format_amount(object.amount, object.currency_id) or ''\">$ 10.00\n"
"                    </span> from <t t-out=\"object.company_id.name or ''\">YourCompany</t>.\n"
"                        <br>\n"
"                        <br>\n"
"                        Do not hesitate to contact us if you have any questions.\n"
"                        <br>\n"
"                        <br>\n"
"                        Best regards,\n"
"                        <t t-if=\"not is_html_empty(user.signature)\">\n"
"                            <br>\n"
"                            <br>\n"
"                            <t t-out=\"user.signature or ''\">--<br>Mitchell Admin\n"
"                            </t>\n"
"                        </t>\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                        Estimado <t t-out=\"object.partner_id.name\"/>\n"
"                        <br/>\n"
"                        <br/>\n"
"                        Gracias por el pago.\n"
"                        Aquí tiene un recibo del mismo <strong><t t-out=\"(object.name or '').replace('/','-')\"/></strong> por un importe de\n"
"                        <strong><t t-out=\"format_amount(object.amount, object.currency_id)\"/></strong> de\n"
"                        <t t-out=\"object.company_id.name\"/>.\n"
"                        <br/>\n"
"                        <br/>\n"
"                        Si requiere cualquier aclaración, no dude en contactarnos.\n"
"                        <br/>\n"
"                        <br/>\n"
"                        Saludos cordiales,\n"
"                        <t t-if=\"user.signature\">\n"
"                        <br/>\n"
"                        <t t-out=\"user.signature\"/>\n"
"                        </t>\n"
"                    </p>\n"
"                </div>\n"
"            "

