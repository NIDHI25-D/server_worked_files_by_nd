<?xml version="1.0" encoding="UTF-8" ?>

<odoo>

    <record id="stock_move_line_tree_view_extended" model="ir.ui.view">
        <field name="name">stock.move.line.tree.view.extended</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_move_line_tree"/>
        <field name="arch" type="xml">
            <field name="product_id" position="after">
                <field name="user_id"/>
            </field>
        </field>
    </record>

    <record id="stock_move_line_form_view_extended" model="ir.ui.view">
        <field name="name">stock.move.line.form.view.extended</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_move_line_form"/>
        <field name="arch" type="xml">
            <field name="lot_id" position="before">
                <field name="user_id"/>
            </field>
        </field>
    </record>

