<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="complexity_levels_search_view" model="ir.ui.view">
            <field name="name">complexity.levels.search.view</field>
            <field name="model">complexity.levels</field>
            <field name="arch" type="xml">
                <search>
                    <field name="name"/>
                    <separator/>
                    <filter string="Archived" name="filter_archived" domain="[('active', '=', False)]"/>
                </search>
            </field>
        </record>

        <record id="complexity_levels_tree_view" model="ir.ui.view">
            <field name="name">complexity.levels.tree.view</field>
            <field name="model">complexity.levels</field>
            <field name="arch" type="xml">
                <list string="Complexity Levels" editable="bottom">
                    <field name="complexity_sequence" widget="handle"/>
                    <field name="name"/>
                    <field name="initial_range_of_lines"/>
                    <field name="end_range_of_lines"/>
                </list>
            </field>
        </record>

        <record id="action_complexity_levels" model="ir.actions.act_window">
            <field name="name">Complexity Levels</field>
            <field name="res_model">complexity.levels</field>
            <field name="view_mode">list</field>
            <field name="view_id" ref="complexity_levels_tree_view"/>
        </record>

        <!--Main Menu that is Dashboard    -->
        <menuitem id="setu_prioritization_of_pickings_menu"
                  name="Prioritization of Pickings"
                  sequence="10"
        />

        <menuitem id="menu_complexity_levels"
                  name="Complexity Levels"
                  parent="setu_prioritization_of_pickings_menu"
                  sequence="1"/>

        <menuitem id="sub_menu_complexity_levels"
                  name="Complexity Levels"
                  parent="setu_prioritization_of_pickings.menu_complexity_levels"
                  action="action_complexity_levels"
                  sequence="1"/>


    </data>
</odoo>