<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="combustible_pin_type_list_view" model="ir.ui.view">
        <field name="name">combustible.pin.type.list</field>
        <field name="model">combustible.pin.type</field>
        <field name="arch" type="xml">
            <list editable="bottom">
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="combustible_pin_type_form_view" model="ir.ui.view">
        <field name="name">combustible.pin.type.form</field>
        <field name="model">combustible.pin.type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="combustible_pin_type_action" model="ir.actions.act_window">
        <field name="name">Combustible Pin Type</field>
        <field name="res_model">combustible.pin.type</field>
        <field name="view_mode">list</field>
    </record>

    <menuitem id="menu_combustible_pin_type" name="Combustible Pin Type" action="combustible_pin_type_action"
              parent="combustible_configuration" sequence="12"/>
</odoo>