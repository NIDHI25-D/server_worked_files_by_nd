<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="ir_cron_module_cancel_invoice_resign" model="ir.cron">
            <field name="name">Cancel invoice resign</field>
            <field eval="True" name="active"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">3</field>
            <field name="interval_type">days</field>
            <field ref="l10n_mx_edi.model_l10n_mx_edi_document" name="model_id"/>
            <field name="state">code</field>
            <field name="code">model.resign_document()</field>
            <field name="priority">100</field>
        </record>

        <record id="ir_cron_custom_auto_send_invoices" model="ir.cron">
            <field name="name">Auto send invoices and sign process</field>
            <field eval="True" name="active"/>
            <field name="model_id" ref="model_account_move"/>
            <field name="state">code</field>
            <field name="code">model._cron_auto_send_invoices_and_sign_process(job_count=5,exclude_ids=[])</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
        </record>

    </data>
</odoo>