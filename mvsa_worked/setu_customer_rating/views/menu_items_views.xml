<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Main menu item : Customer Score-->
        <menuitem name="Customer Score"
                  web_icon="setu_customer_rating,static/description/icon.png"
                  id="score_table_menu"/>

        <!-- Menu item : Customer Score-->
        <menuitem sequence="1"
                  name="Customer Score"
                  id="customer_score_menu"
                  parent="setu_customer_rating.score_table_menu"
                  action="setu_customer_rating.customer_score_act_window"/>

        <!-- Menu item : Reports-->
        <menuitem id="setu_partner_rating_history_main_menu"
                  name="Reports" parent="setu_customer_rating.score_table_menu"
                  sequence="6"/>

        <!-- Child Menu item of Configuration : Reports-->
        <menuitem id="setu_partner_rating_history_child_menu"
                  name="Rating History"
                  action="setu_customer_rating.setu_partner_rating_history_action"
                  parent="setu_customer_rating.setu_partner_rating_history_main_menu"
                  sequence="9"/>

        <!-- Menu item : Configuration-->
        <menuitem name="Configuration"
