                      id="customer_score_settings_menu"
                      action="setu_customer_rating.customer_score_config_settings"/>

            <!-- Child Menu item of Configuration : Score Rules-->
            <menuitem sequence="2"
                      name="Score Rules"
                      id="score_rules_menu"
                      action="setu_customer_rating.score_conf_act_window"/>

            <!-- Child Menu item of Configuration : Rating Table-->
            <menuitem name="Rating Table"
                      id="customer_rating_menu"
                      action="setu_customer_rating.customer_rating_act_window"
                      sequence="3"/>

            <!-- Child Menu item of Configuration : Score Conf creator-->
            <menuitem name="Score Conf Creator"
                      id="score_conf_creator_menu"
                      action="setu_customer_rating.score_conf_creator_act_window"
                      sequence="4"/>
