<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="res_config_settings_sale_marvelsa"	model="ir.ui.view">
		<field name="name">res.config.settings.view.sale.marvelsa</field>
		<field name="model">res.config.settings</field>
		<field name="priority" eval="40" />
		<field name="inherit_id" ref="base.res_config_settings_view_form" />
		<field name="arch" type="xml">
			<xpath expr="////form/app[@name='sale_management']" position="inside">
				<h2>Sales Marvelsa</h2>
				<div class="row mt16 o_settings_container">
					<div class="col-xs-12 col-md-6 o_setting_box">
						<div class="o_setting_right_pane">
			                <label string="Customer Agrobolder" for="customer_agrobolder_id"/>
			                <div class="text-muted">
			                    
			                </div>
			                <div class="mt16">
			                    <field name="customer_agrobolder_id"/>
			                </div>
			            </div>
					</div>
				</div>
			</xpath>
		</field>
	</record>

</odoo>