<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="website_text_delivery_carrier_view_form" model="ir.ui.view">
        <field name="name">website.text.delivery.carrier.view.form</field>
        <field name="model">delivery.carrier</field>
        <field name="inherit_id" ref="delivery.view_delivery_carrier_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='free_over_amount']" position="after">
                <field name="display_message_on_website"/>
            </xpath>
        </field>
    </record>
</odoo>