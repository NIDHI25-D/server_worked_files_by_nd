<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!-- Inherit Form View to Modify it -->
    <record id="add_field_is_enable_create_picking" model="ir.ui.view">
        <field name="name">add.field.is_enable_create_picking</field>
        <field name="model">stock.picking.type</field>
        <field name="inherit_id" ref="stock.view_picking_type_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='create_backorder']" position="after">
                <field name="is_enable_create_picking"/>
            </xpath>
        </field>
    </record>
</odoo>