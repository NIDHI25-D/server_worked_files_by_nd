<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data noupdate="1">

        <record model="ir.module.category" id="module_category_crm">
            <field name="name">CRM</field>
            <field name="description">Helps you manage your crm users</field>
            <!--            <field name="sequence">16</field>-->
        </record>
        <record id="group_crm_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="module_category_crm"/>
        </record>
        <record id="group_crm_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_category_crm"/>
            <field name="implied_ids" eval="[(4, ref('group_crm_user'))]"/>
        </record>
    </data>
    <record id="group_crm_user_to_change_stage_manually" model="res.groups">
        <field name="name">Enable to change CRM Lead stage manually</field>
    </record>
</odoo>
