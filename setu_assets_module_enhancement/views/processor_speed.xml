<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="processor_speed_view_tree" model="ir.ui.view">
            <field name="name">processor_speed_view_tree</field>
            <field name="model">processor.speed</field>
            <field name="arch" type="xml">
                <list string="processor_speed_tree" editable="bottom">
                    <field name="name" required="1"/>
                </list>
            </field>
        </record>

        <record id="processor_speed_action" model="ir.actions.act_window">
            <field name="name">Processor Speed</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">processor.speed</field>
            <field name="view_mode">list</field>
            <field name="view_id" ref="processor_speed_view_tree"/>
        </record>
    </data>
</odoo>