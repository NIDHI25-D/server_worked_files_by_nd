<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="processor_model_view_tree" model="ir.ui.view">
            <field name="name">processor_model_view_tree</field>
            <field name="model">processor.model</field>
            <field name="arch" type="xml">
                <list string="processor_model_tree" editable="bottom">
                    <field name="name" required="1"/>
                </list>
            </field>
        </record>

        <record id="processor_model_action" model="ir.actions.act_window">
            <field name="name">Processor Model</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">processor.model</field>
            <field name="view_mode">list</field>
            <field name="view_id" ref="processor_model_view_tree"/>
        </record>
    </data>
</odoo>